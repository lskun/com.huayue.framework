<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 数据映射配置 -->
<mapper namespace="entitiesMap">
	<resultMap type="User" id="userMap">
		<id property="id" column="id"/>
		<result property="name" column="user_name"/>
		<result property="password" column="password"/>
		<result property="deptId" column="depid"/>
		<result property="permission" column="permission"/>
		<result property="createTime" column="create_time"/>
		<result property="modifiedTime" column="modified_time"/>
	</resultMap>
	
	<resultMap type="Acticle" id="acticleMap">
		<id property="id" column="id"/>
		<result property="name" column="name"/>
		<result property="author" column="author"/>
		<result property="countryId" column="country_id"/>
		<result property="genre" column="genre"/>
		<result property="presentation" column="presentation"/>
		<result property="origin" column="origin"/>
		<result property="firConId" column="first_content_id"/>
		<result property="secConId" column="second_content_id"/>
		<result property="comment" column="comment"/>
		<result property="directory" column="directory"/>
		<result property="suffix" column="suffix"/>
		<result property="size" column="size"/>
		<result property="famousMasterpiece" column="famous_masterpiece"/>
		<result property="times" column="times"/>
		<result property="version" column="version"/>
		<result property="createTime" column="create_time"/>
		<result property="userId" column="user_id"/>
		<result property="updateTime" column="update_time"/>		
	</resultMap>
</mapper>