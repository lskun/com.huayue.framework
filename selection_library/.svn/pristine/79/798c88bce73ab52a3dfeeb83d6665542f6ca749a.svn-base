package com.huayue.library.domain;

import java.util.Date;
import java.util.List;

/**
 * @author lsk0414
 * 
 */
public class Book implements java.io.Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	private int id;
	
	//书名
	private String name;

	// 出版社
	private String publisher;
	
	// 作者
	private String author;

	// 文章集合
	private List<Acticle> acticles;
	
	//文章总数
	private int acticleCount;
	
	// 创建人
	private int userId;
	
	//创建时间
	private long createTime;

	// 出版日期
	private long publishDate;

	// 类别ID
	private int categoryId;
	
	// 书籍描述
	private String bookDescription;
	
	//类别层级关系
	private String hierarchicalNode;
	
	//long -> date (createTime)
	//private Date createTimeText;
	
	//long -> date (publishdate)
	//private Date publishDateText;

	/**
	 * @return the id
	 */
	public int getId() {
		return id;
	}

	/**
	 * @param id
	 *            the id to set
	 */
	public void setId(int id) {
		this.id = id;
	}

	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}

	/**
	 * @param name
	 *            the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}

	/**
	 * @return the publisher
	 */
	public String getPublisher() {
		return publisher;
	}

	/**
	 * @param publisher
	 *            the publisher to set
	 */
	public void setPublisher(String publisher) {
		this.publisher = publisher;
	}


	/**
	 * @return the acticles
	 */
	public List<Acticle> getActicles() {
		return acticles;
	}

	/**
	 * @param acticles the acticles to set
	 */
	public void setActicles(List<Acticle> acticles) {
		this.acticles = acticles;
	}

	/**
	 * @return the userId
	 */
	public int getUserId() {
		return userId;
	}

	/**
	 * @param userId
	 *            the userId to set
	 */
	public void setUserId(int userId) {
		this.userId = userId;
	}

	/**
	 * @return the createTime
	 */
	public long getCreateTime() {
		return createTime;
	}

	/**
	 * @param createTime
	 *            the createTime to set
	 */
	public void setCreateTime(long createTime) {
		this.createTime = createTime;
	}

	/**
	 * @return the publishDate
	 */
	public long getPublishDate() {
		return publishDate;
	}

	/**
	 * @param publishDate
	 *            the publishDate to set
	 */
	public void setPublishDate(long publishDate) {
		this.publishDate = publishDate;
	}

	/**
	 * @return the categoryId
	 */
	public int getCategoryId() {
		return categoryId;
	}

	/**
	 * @param categoryId
	 *            the categoryId to set
	 */
	public void setCategoryId(int categoryId) {
		this.categoryId = categoryId;
	}

	/**
	 * @return the author
	 */
	public String getAuthor() {
		return author;
	}

	/**
	 * @param author the author to set
	 */
	public void setAuthor(String author) {
		this.author = author;
	}

	/**
	 * @return the categoryDescription
	 */
	public String getBookDescription() {
		return bookDescription;
	}

	/**
	 * @param categoryDescription the categoryDescription to set
	 */
	public void setBookDescription(String categoryDescription) {
		this.bookDescription = categoryDescription;
	}

	/**
	 * @return the hierarchicalNode
	 */
	public String getHierarchicalNode() {
		return hierarchicalNode;
	}

	/**
	 * @param hierarchicalNode the hierarchicalNode to set
	 */
	public void setHierarchicalNode(String hierarchicalNode) {
		this.hierarchicalNode = hierarchicalNode;
	}	

	/**
	 * @return the acticleCount
	 */
	public int getActicleCount() {
		return acticleCount;
	}

	/**
	 * @param acticleCount the acticleCount to set
	 */
	public void setActicleCount(int acticleCount) {
		this.acticleCount = acticleCount;
	}

	/**
	 * @return the createTimeText
	 */
	public Date getCreateTimeText() {
		return new Date(createTime);
	}

	/**
	 * @return the publishDateText
	 */
	public Date getPublishDateText() {
		return new Date(publishDate);
	}	
	
}
