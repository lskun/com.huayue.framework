/* CSS Document */
#header {
	height: 35px;
}
#header.org{
	height: 54px;
}
#header p {
	height: 40px;
	background: url(../images/header_bg.png) left bottom;
}
#header.user p {
	height: 54px;
	margin: 0px 31px;
}

.logo {
	float: left;
	margin: 8px 0px 0px 10px;
}
.user .logo{ 
	margin-top: 13px;
}
.user .menu {
	padding: 16px 20px 0px 20px;
}

.menu {
	color:#2a81C9;
	float: left;
	padding: 8px 20px 0px 20px;
}

.menu a {
	line-height: 1;
	text-decoration: none;
	color: #2a81C9;
	padding: 0px 15px;
}
.proj .menu a{
	padding: 0px 10px;
}
.menu a.selected{
	font-weight: bold;
	color: #ff5a00;
}
.menu a:hover {
	color: #ff5a00;
}

.user .tools {
	height: 30px;
	padding: 18px 20px 0px 40px;
	word-spacing: 1em;
}

.tools {
	height: 28px;
	background: url(../images/tools_bg.png) no-repeat;
	float: right;
	padding: 7px 15px 0px 35px;
	word-spacing: 1em;
}

.tools a {
	color: #2D83CB;
	text-decoration: none;
	margin:0px 10px;
}

.tools a strong {
	color: #FF3300;
	font-weight: normal;
}

.tools a:hover {
	color: #00C0FF;
}

.tools img {
	width: 16px;
	height: 16px;
	vertical-align: middle;
	margin: -2px 6px 0px 0px;
}

.top {
	top: 0px;
	right: 0px;
	position: absolute;
}

.left {
	float: left;
}

.right {
	float: right;
}

/*
 * Description: 标准化浏览器的样式
 *	 - 	《超越CSS：Web设计艺术精髓》 P.157
 */
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,p,blockquote,th,td,a {
	font-family: Arial, sans-serif, Helvetica, Tahoma;
	line-height: 1.5;
	margin: 0px;
	padding: 0px;
}

#wrapper {
	width: 100%;
	min-height: 100%;
	_height: auto !important;
	_min-height: 100%;
	_height: 100%;
}

html,body {
	height: 100%;
}

h1,h2,h3,h4,h5,h6 {
	font-size: 100%;
}

ul.line2h li {
	line-height: 2;
}

ol,ul {
	list-style: none;
}

address,caption,cite,code,dfn,em,strong,th,var {
	font-style: normal;
	font-weight: normal;
}

.hide {
	display: none;
}

table {
	border-collapse: collapse;
	border-spacing: 0;
}

fieldset,img,abbr {
	border: 0;
}

caption,th {
	text-align: left;
}

q:before,q:after {
	content: '';
}

a {
	text-decoration: none;
	cursor: pointer;
}

a:focus,input:focus {
	outline: none;
}

IMG {
	border-width: 0;
}

#main_menu_id,#menu_type,#PAGE_PROJ_ID {
	display: none;
}

div.branding {
	float: left;
	width: 200px;
}

div.ad {
	float: right;
	z-index: 100px;
}

#wlcDiv.float {
	float: right;
}

.service {
	text-align: left;
	margin: 10px;
	font-size: 14px;
}

/*------------------------------ menu -------------------------------------*/
ul.menu {
	list-style: none;
	background: #bbb;
	height: 30px;
	margin: 0px;
}

ul.menu.simple {
	background: #fff;
}

ul.menu li {
	float: left;
	background: #CCC;
	margin-left: 2px;
}

ul.menu li.logo {
	background: #fff;
	margin-left: 0px;
}

ul.menu li.selected a:link,ul.menu li.selected a:visited {
	color: #ff0;
	background: #22a;
}

ul.menu li a {
	padding-left: 10px;
	padding-right: 10px;
	display: block;
	text-align: center;
	text-decoration: none;
	letter-spacing: 4px;
	line-height: 30px;
}

ul.menu.simple li a {
	padding-left: 8px;
	padding-right: 8px;
	letter-spacing: 2px;
}

ul.menu li a:link,ul.menu li a:visited {
	color: #ddd;
	background: #33f no-repeat 5px 12px;
}

ul.menu li a:hover {
	color: #333;
	background: #ddd;
}

ul.menu li.logo a {
	background: #fff;
	padding: 0px;
}
.user.quik_menu{
	top: 39px;
}
.quik_menu {
	border: #8BCCE9 1px solid;
	background: #FFFFFF;
	padding: 3px;
	display: none;
	position: absolute;
	z-index: 999;
	text-align: left;
	top: 25px;
}

.quik_menu a {
	text-decoration: none;
	color: #2D83CB;
	display: block;
	padding: 6px;
	min-width: 100px;
	max-width: 180px;
}
.quik_menu a.cur{
	font-weight: bold;
	color: #2A80C8;
}
.quik_menu a:hover {
	background: #2D83CB;
	color: #FFF;
	display: block;
	padding: 6px;
}


/*------------------------------ public -------------------------------------*/
body {
