    background: url("../bg/btn_bg.png") no-repeat scroll 0 -25px transparent;
    border: 0 none;
    color: #FFFFFF;
    height: 25px !important;
    line-height: 25px;
    width: 80px;
}

td input[type=text].big {
	width: 90%;
}

td input[type=text].small,td input[type=password].small,input[type=text].issue {
	max-width: 150px;
}

td input[type=text].normal,td input[type=password].normal {
