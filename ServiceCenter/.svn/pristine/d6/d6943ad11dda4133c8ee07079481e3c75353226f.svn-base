package com.huayue.crm.domain;

public class Client {
	
	private String deliverprovince;		//省
		
	private String delivercity;			//市
	
	private String deliverarea;			//地级市/县/区
	
	private String unit;			//单位
	
	private long registerTime;		//录入时间
	
	private long id ;				//ID
	
	private String name;			//姓名
	
	private String sex;				//性别
	
	private String callName;		//称呼
	
	private String duty;			//职务
	
	private String phone;			//办公电话
	
	private String mobile;			//手机号码
	
	private String email;			//电子邮箱
	
	private String fax;				//传真
	
	private String nation;			//民族
	
	private String address;			//地址
	
	private String postCode;		//邮编
	
	private long categoryId;			//客户类别
	
	private String joinedActivity;	//参加过的活动
	
	private String remark;			//补充说明
	
	public Client(){}

	public String getDeliverprovince() {
		return deliverprovince;
	}

	public void setDeliverprovince(String deliverprovince) {
		this.deliverprovince = deliverprovince;
	}

	public String getDelivercity() {
		return delivercity;
	}

	public void setDelivercity(String delivercity) {
		this.delivercity = delivercity;
	}

	public String getDeliverarea() {
		return deliverarea;
	}

	public void setDeliverarea(String deliverarea) {
		this.deliverarea = deliverarea;
	}

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getSex() {
		return sex;
	}

	public void setSex(String sex) {
		this.sex = sex;
	}

	public String getCallName() {
		return callName;
	}

	public void setCallName(String callName) {
		this.callName = callName;
	}

	public String getDuty() {
		return duty;
	}

	public void setDuty(String duty) {
		this.duty = duty;
	}

	public String getPhone() {
		return phone;
	}

	public void setPhone(String phone) {
		this.phone = phone;
	}

	public String getMobile() {
		return mobile;
	}

	public void setMobile(String mobile) {
		this.mobile = mobile;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getFax() {
		return fax;
	}

	public void setFax(String fax) {
		this.fax = fax;
	}

	public String getNation() {
		return nation;
	}

	public void setNation(String nation) {
		this.nation = nation;
	}

	public String getAddress() {
		return address;
	}

	public void setAddress(String address) {
		this.address = address;
	}

	public String getPostCode() {
		return postCode;
	}

	public void setPostCode(String postCode) {
		this.postCode = postCode;
	}

	public long getCategoryId() {
		return categoryId;
	}

	public void setCategoryId(long categoryId) {
		this.categoryId = categoryId;
	}

	public String getJoinedActivity() {
		return joinedActivity;
	}

	public void setJoinedActivity(String joinedActivity) {
		this.joinedActivity = joinedActivity;
	}

	public String getUnit() {
		return unit;
	}

	public void setUnit(String unit) {
		this.unit = unit;
	}

	public long getRegisterTime() {
		return registerTime;
	}

	public void setRegisterTime(long registerTime) {
		this.registerTime = registerTime;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	};
	
	
}
